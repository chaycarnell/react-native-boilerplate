{
  "name": "rnbp",
  "description": "A quick start template for RN apps",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "android": "react-native run-android ENVFILE=.env --variant=debug",
    "android:release": "react-native run-android ENVFILE=.env.release --variant=release",
    "adb:reverse:debug": "adb reverse tcp:8081 tcp:8081",
    "adb:reverse:server": "adb reverse tcp:4000 tcp:4000",
    "ios": "npm run ios:pods && react-native run-ios ENVFILE=.env --configuration Debug",
    "ios:release": "npm run ios:pods && react-native run-ios ENVFILE=.env.release --configuration Release",
    "ios:small": "npm run ios:pods && react-native run-ios --simulator 'iPhone SE (2nd generation)'",
    "ios:xsmall": "npm run ios:pods && react-native run-ios --simulator 'iPhone SE (1st generation)'",
    "ios:ipad": "npm run ios:pods && react-native run-ios --simulator 'iPad (7th generation)'",
    "ios:ipadxl": "npm run ios:pods && react-native run-ios --simulator 'iPad Pro (12.9-inch) (4th generation)'",
    "nuke": "rm -rf node_modules && npm install && npm run ios:pods --reset-cache && npm run start --reset-cache",
    "ios:pods": "cd ios && pod install && cd ..",
    "ios:pods:update": "cd ios && pod install --repo-update && cd ..",
    "start": "react-native start",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,json}",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "upgrade:rn": "npx react-native upgrade",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@apollo/react-hooks": "^4.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "@sentry/react-native": "^2.4.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-bootsplash": "^3.2.0",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.0.0",
    "react-native-url-polyfill": "^1.3.0",
    "styled-components": "^5.2.3",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/react-native": "^0.63.4",
    "@types/styled-components": "^5.1.9",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-native": "^3.10.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "prettier": "^2.2.1",
    "react-native-rename": "^2.7.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-url-polyfill|react-native-config|react-navigation|@react-navigation/.*|@react-navigation/native|@react-navigation/stack|react-native-gesture-handler|react-native-iphone-x-helper)/)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom"
  }
}
