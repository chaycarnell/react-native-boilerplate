{
  "name": "rnbp",
  "description": "A quick start boilerplate for RN apps",
  "version": "1.0.3",
  "license": "ISC",
  "scripts": {
    "android": "react-native run-android --variant=debug",
    "android:release": "react-native run-android --variant=release",
    "adb:reverse:debug": "adb reverse tcp:8081 tcp:8081",
    "adb:reverse:server": "adb reverse tcp:4000 tcp:4000",
    "ios": "npm run ios:pods && react-native run-ios --configuration Debug",
    "ios:release": "npm run ios:pods && react-native run-ios --configuration Release",
    "ios:small": "npm run ios:pods && react-native run-ios --simulator 'iPhone SE (2nd generation)'",
    "ios:xsmall": "npm run ios:pods && react-native run-ios --simulator 'iPhone SE (1st generation)'",
    "ios:ipad": "npm run ios:pods && react-native run-ios --simulator 'iPad (7th generation)'",
    "ios:ipadxl": "npm run ios:pods && react-native run-ios --simulator 'iPad Pro (12.9-inch) (4th generation)'",
    "clean:builds": "cd android && ./gradlew clean && cd ../ && cd ios && xcodebuild clean && cd ../",
    "nuke": "npx kill-port 8081 && rm -rf node_modules && npm run clean && npm install && npm run ios:pods && npm run start:reset",
    "ios:pods": "cd ios && pod install && cd ..",
    "ios:pods:update": "cd ios && pod install --repo-update && cd ..",
    "start:reset": "watchman watch-del-all && react-native start --reset-cache",
    "start": "react-native start",
    "test": "jest --watchAll --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:update": "jest --updateSnapshot",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,json}",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "upgrade:rn": "npx react-native upgrade",
    "prepare": "husky install",
    "rename": "npx kill-port 8081 && npm install react-native-rename -g && npx react-native-rename 'RNBP' && npm run ios:pods && npm run start:reset"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/react-hooks": "^4.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "i18next": "^20.3.1",
    "react": "17.0.2",
    "react-i18next": "^11.11.0",
    "react-native": "0.64.2",
    "react-native-bootsplash": "^3.2.3",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.3.0",
    "react-native-url-polyfill": "^1.3.0",
    "styled-components": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/runtime": "^7.14.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/react-native": "^0.64.10",
    "@types/styled-components": "^5.1.10",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.2.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.1",
    "react-native-rename": "^2.9.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.3.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-url-polyfill|react-native-config|react-navigation|@react-navigation/.*|@react-navigation/native|@react-navigation/stack|react-native-gesture-handler|react-native-iphone-x-helper|react-native-screens)/)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "text-summary",
      "cobertura"
    ]
  }
}
