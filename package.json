{
  "name": "starter-app",
  "version": "0.73.2",
  "private": true,
  "scripts": {
    "adb:reverse:debug": "adb reverse tcp:8081 tcp:8081",
    "android": "react-native run-android",
    "commit": "cz",
    "ios": "yarn ios:pods && react-native run-ios",
    "ios:pods": "cd ios && pod install && cd ..",
    "ios:pods:update": "cd ios && pod install --repo-update && cd ..",
    "lint": "yarn types:check && eslint ./src --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json} && npx prettier --write *.{js,json}",
    "reset": "watchman watch-del-all && react-native start --reset-cache",
    "sort:package": "npx sort-package-json",
    "start": "react-native start",
    "test": "jest --watchAll --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:update": "jest --updateSnapshot",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.9.2",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/stack": "6.3.20",
    "graphql": "16.8.1",
    "i18next": "23.8.2",
    "intl-pluralrules": "2.0.1",
    "react": "18.2.0",
    "react-i18next": "14.0.2",
    "react-native": "0.73.3",
    "react-native-bootsplash": "5.3.0",
    "react-native-config": "1.5.1",
    "react-native-gesture-handler": "2.14.1",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@react-native/babel-preset": "^0.73.20",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.4",
    "@react-native/typescript-config": "^0.74.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "^18.2.53",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-ft-flow": "^3.0.4-alpha-0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=21"
  }
}
